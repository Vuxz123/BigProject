<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="pane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="521.0" prefWidth="1019.0"
            style="-fx-background-color: #222222;" xmlns="http://javafx.com/javafx/18">
    <children>
        <Label fx:id="songLabel" alignment="CENTER" layoutX="182.0" layoutY="112.0" prefHeight="101.0" prefWidth="655.0"
               text="MP3 Player" textFill="#00ff1a">
            <font>
                <Font size="65.0"/>
            </font>
        </Label>
        <ProgressBar fx:id="songProgressBar" layoutX="159.0" layoutY="225.0" prefHeight="18.0" prefWidth="655.0"
                     progress="0.0"/>
        <Button fx:id="playButton" layoutX="159.0" layoutY="264.0" mnemonicParsing="false" onAction="#playMedia"
                prefHeight="46.0" prefWidth="85.0" text="PLAY">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="pauseButton" layoutX="245.0" layoutY="264.0" mnemonicParsing="false" onAction="#pauseMedia"
                prefHeight="46.0" prefWidth="85.0" text="PAUSE">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="resetButton" layoutX="327.0" layoutY="264.0" mnemonicParsing="false" onAction="#resetMedia"
                prefHeight="46.0" prefWidth="85.0" text="RESET">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="previousButton" layoutX="413.0" layoutY="264.0" mnemonicParsing="false" onAction="#previousMedia"
                prefHeight="46.0" prefWidth="85.0" text="PREVIOUS">
            <font>
                <Font size="13.0"/>
            </font>
        </Button>
        <Button fx:id="nextButton" layoutX="499.0" layoutY="264.0" mnemonicParsing="false" onAction="#nextMedia"
                prefHeight="46.0" prefWidth="85.0" text="NEXT">
            <font>
                <Font size="13.0"/>
            </font>
        </Button>
        <Slider fx:id="volumeSlider" layoutX="664.0" layoutY="280.0" max="100.0" prefHeight="14.0" prefWidth="149.0"
                value="50.0"/>
        <Button fx:id="backToOptionButton" layoutX="413.0" layoutY="373.0" mnemonicParsing="false"
                onAction="#backToOption" text="BACK TO OPTION"/>
        <Button fx:id="backToMenuButton" layoutX="413.0" layoutY="325.0" mnemonicParsing="false" onAction="#backToMenu"
                text="BACK TO MENU "/>
        <ComboBox fx:id="speedBox" layoutX="585.0" layoutY="264.0" onAction="#changeSpeed" prefHeight="46.0"
                  prefWidth="79.0" promptText="SPEED"/>
        <Button fx:id="backToGameButton" layoutX="416.0" layoutY="426.0" mnemonicParsing="false" onAction="#backToGame"
                text="BACK TO GAME "/>
    </children>
</AnchorPane>
